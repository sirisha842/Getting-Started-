set serveroutput on;
declare
no number(10);
name varchar2(10);
begin
select empno,ename into no,name from emp where ename='MARTIN';
insert into empd values(no,name);
end;

set serveroutput on;
declare
no number(10);
name varchar2(10);
name1 varchar2(10):='&NAMe';
begin
select empno,ename into no,name from emp where ename='MARTIN';
if name1=name then insert into empd values(no,name);
end if;
end;

